<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script
    src="https://code.jquery.com/jquery-3.2.1.min.js"
    integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
    crossorigin="anonymous"></script>

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">

    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>

    <script type="text/javascript" src="static/login.js">

    </script>


    <title></title>
  </head>
  <body>
    <div class="row">
      <div class="col s12">
        <ul class="tabs">
          <li class="tab col s6"><a href="#test1">Traveler</a></li>
          <li class="tab col s6"><a href="#test2">Local</a></li>
        </ul>
      </div>

      <!-- traveler login -->
      <div id="test1" class="col s12">
        <div class="row">
          <div class="col s10 m8 l6 offset0-s1 offset-m2 offset-l3 card ">
            <h3 class="center-align">Traveler Login</h3>
            <form class="" action="/local/login" method="post">


            <label for="username">Username</label>
            <input type="text" name="username">
            <label for="password">Password</label>
            <input type="password" name="password" value="">
            <input type="submit" name="" value="Lets's go!" class="btn">


          </form>
          <a href="/traveler/register">New here?</a>
          </div>

        </div>
      </div>

      <!-- local login -->
      <div id="test2" class="col s12">
        <div class="row">
          <div class="col s10 m8 l6 offset0-s1 offset-m2 offset-l3 card">
            <h3 class="center-align">Local Login</h3>
            <form class="" action="/local/login" method="post">


            <label for="username">Username</label>
            <input type="text" name="username">
            <label for="password">Password</label>
            <input type="password" name="password" value="">
            <input type="submit" name="" value="Lets's go!" class="btn">


          </form>
          <a href="/local/register">New here?</a>
          </div>
      </div>
    </div>

  </body>
</html>
